{
  "name": "minecards-renderer",
  "version": "0.1.6",
  "description": "A library for displaying interactive 3D collectible cards for Telegram bot Minecrads and their web interface.",
  "type": "module",
  "main": "./dist/minecards-renderer.umd.js",
  "module": "./dist/minecards-renderer.es.js",
  "exports": {
    ".": {
      "import": "./dist/minecards-renderer.es.js",
      "require": "./dist/minecards-renderer.umd.js"
    },
    "./style.css": "./dist/minecards-renderer.css"
    },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.config.demo.js", 
    "build": "vite build", 
    "preview": "vite preview",
    "deploy-demo": "vite build --config vite.config.demo.js && gh-pages -d dist-demo"
  },
  "dependencies": {
    "skin3d": "^0.0.12"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "vite": "^7.0.6",
    "vite-plugin-copy": "^0.1.6"
  },
  "keywords": [
    "card",
    "3d",
    "vanilla-js",
    "collectible",
    "skin3d"
  ],
  "author": "Bezlikiy",
  "license": "MIT"
}
