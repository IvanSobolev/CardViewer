{
  "name": "minecards-renderer",
  "version": "0.1.0",
  "description": "A library for displaying interactive 3D collectible cards for Telegram bot Minecrads and their web interface.",
  "type": "module",
  "main": "./dist/cool-3d-card-viewer.umd.cjs",
  "module": "./dist/cool-3d-card-viewer.js",
  "exports": {
    ".": {
      "import": "./dist/cool-3d-card-viewer.js",
      "require": "./dist/cool-3d-card-viewer.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build --config vite.config.demo.js && gh-pages -d dist-demo"
  },
  "dependencies": {
    "skin3d": "^0.0.12"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "vite": "^7.0.6"
  },
  "keywords": [
    "card",
    "3d",
    "vanilla-js",
    "collectible",
    "skin3d"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "Bezlikiy",
  "license": "MIT"
}
